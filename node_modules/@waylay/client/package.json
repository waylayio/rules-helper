{
  "name": "@waylay/client",
  "version": "2.37.0",
  "description": "Waylay API client for Node and the browser",
  "main": "index.js",
  "module": "dist/waylay.esm.js",
  "browser": "dist/waylay.umd.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "test": "run-p -sn test:*",
    "test:lint": "standard | snazzy",
    "test:src": "jest",
    "build": "rollup -c",
    "version": "yarn build && conventional-changelog -i CHANGELOG.md -s -r 0 -p @spyke/conventional-changelog-preset && git add CHANGELOG.md",
    "doc": "yarn documentation build lib/waylay.js -f html -c documentation.yml -o docs-dist/",
    "coverage": "open ./coverage/lcov-report/index.html"
  },
  "engines": {
    "node": "> 6.0.0"
  },
  "keywords": [
    "waylay",
    "sdk",
    "client"
  ],
  "author": "Gilles De Mey <gilles@waylay.io>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "debug": "^3.1.0",
    "form-data": "^3.0.0",
    "is-buffer": "^2.0.2",
    "jwt-decode": "^2.2.0",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0",
    "prepend-http": "^2.0.0",
    "query-string": "^5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-json": "^4.1.0",
    "@spyke/conventional-changelog-preset": "^1.0.5",
    "browserslist": "^4.16.3",
    "conventional-changelog-cli": "^2.1.1",
    "core-js": "^3.6.5",
    "documentation": "^13.2.1",
    "husky": "^3.0.9",
    "jest": "^25.1.0",
    "jsonwebtoken": "^8.5.1",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.2",
    "object-values": "^1.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "snazzy": "^7.0.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": [
      "dist/"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "dist/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "bail": true,
    "testEnvironment": "node"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": ">0.25%, not op_mini all"
}
